# Signer
XMTP_WALLET_KEY=

# Database
XMTP_DB_DIRECTORY=.database
XMTP_DB_ENCRYPTION_KEY=

# Deployment
XMTP_ENV=dev

# Debugging
XMTP_FORCE_DEBUG_LEVEL=Error
XMTP_FORCE_DEBUG=true

# Pinata IPFS File Upload
PINATA_GATEWAY=
PINATA_JWT=

# Agent specific environment variables
# Backend selection - openai|perplexity|anthropic
AI_BACKEND=openai
OPENAI_API_KEY=your-api-key-here
OPENAI_MODEL=gpt-4o-mini
PERPLEXITY_API_KEY=your-api-key-here
PERPLEXITY_MODEL=llama-3.1-sonar-small-128k-online
ANTHROPIC_API_KEY=your-api-key-here
ANTHROPIC_MODEL=claude-sonnet-4-20250514

# Resolved cache TTL for YouTube videos
YOUTUBE_CACHE_TTL_MS=3600000
